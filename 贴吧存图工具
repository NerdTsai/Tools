#coding=utf-8
import urllib
import re

reg = 'original="(http://imgsrc.baidu.com/.*?\.jpg)"'

def getHtml(url):
	page = urllib.urlopen(url)
	html = page.read()
	return html
	
def getImg(html):
	imgre = re.compile(reg)
	imglist = re.findall(imgre,html)
	x = 0
	for imgurl in imglist:
		urllib.urlretrieve(imgurl,'%s.jpg' % x)
		x+=1

html = getHtml("http://tieba.baidu.com/f?kw=%C2%CC%BE%DE%C8%CB&fr=index")

print getImg(html)
