#coding=utf-8
import urllib
import re

reg = 'original="(http://imgsrc.baidu.com/.*?\.jpg)"'

def getHtml(url):
	page = urllib.urlopen(url)
	html = page.read()
	return html
	
def getImg(html):
	imgre = re.compile(reg)
	imglist = re.findall(imgre,html)
	x = 0
	for imgurl in imglist:
		urllib.urlretrieve(imgurl,'%s.jpg' % x)
		x+=1

html = getHtml("http://tieba.baidu.com/f?ie=utf-8&kw=%E8%84%9A%E6%A8%A1")

print getImg(html)
